<head>
    <meta charset="UTF-8">
    <title>student</title>
    <script src="~/jQuery easyUI/jquery.min.js"></script>
    <script src="~/jQuery content custom scroller/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/jQuery easyUI/jquery.easyui.min.js"></script>
    <script src="~/jQuery easyUI/easyui-lang-zh_CN.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/bootstrap-3.3.7-dist/js/bootstrap.js"></script>
    <link rel="stylesheet" href="~/jQuery easyUI/themes/material-teal/easyui.css">
    <link rel="stylesheet" href="~/font-awesome-4.7.0/css/font-awesome.css">
    <link rel="stylesheet" href="~/CSS/s_layout.css">
    <link href="//cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/bootstrap-3.3.7-dist/css/bootstrap.css">
    <link rel="stylesheet" href="~/bootstrap-3.3.7-dist/css/bootstrap-theme.css">
    <script>
        $.getJSON("/Student/Practice_info", function (result) {
            $("li ul").html("");
            $.each(result.pracinfo, function (i, field) {
                var str = '<div id="_easyui_tree_5" class="tree-node"><span class="tree-indent"></span><span class="tree-indent"></span><span class="tree-icon tree-file fa fa-file-text-o"></span><span class="tree-title"><a href="/Stuhome/Practice_info/' + field.ec_id + '">' + field.name + '</a></span></div>';
                $("li ul:first").append(str);
            });
            $.each(result.examinfo, function (i, field) {
                //var str = '<div id="_easyui_tree_5" class="tree-node"><span class="tree-indent"></span><span class="tree-indent"></span><span class="tree-icon tree-file fa fa-file-text-o"></span><span class="tree-title"><a href="/Stuhome/Exam/' + field.ec_id + '">' + field.name + '</a></span></div>'
                var str = '<div id="_easyui_tree_5" class="tree-node"><span class="tree-indent"></span><span class="tree-indent"></span><span class="tree-icon tree-file fa fa-file-text-o"></span><span class="tree-title"><a href="/Stuhome/Exam_explanation/'+ field.ec_id + '">' + field.name + '</a></span></div>'
                $("li ul:eq(1)").append(str);
            })
            $.each(result.examFinishinfo, function (i, field) {
                var str = '<div id="_easyui_tree_5" class="tree-node"><span class="tree-indent"></span><span class="tree-indent"></span><span class="tree-icon tree-file fa fa-file-text-o"></span><span class="tree-title"><a href="/Stuhome/Exam_feedback/' + field.ec_id + '">' + field.name + '</a></span></div>'
                //var str = '<div id="_easyui_tree_5" class="tree-node"><span class="tree-indent"></span><span class="tree-indent"></span><span class="tree-icon tree-file fa fa-file-text-o"></span><span class="tree-title"><a href="/Stuhome/ExamExplanation">' + field.name + '</a></span></div>'
                $("li ul:last").append(str);
            })
            $.parser.parse();
        })
    </script>
</head>
<body class="easyui-layout">
    <div data-options="region:'north',border:false" style="height:60px;padding:10px;background-color: #011935">
        <div style="width: 300px;margin-top: 5px">
            <!--<img id="schoolbadge" src="IMG/校徽（白）.png" height="30" width="30">-->
