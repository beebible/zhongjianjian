        </div>
        <a id="exit" href="/Stuhome/Exit" target="_self"><i class="fa fa-sign-out fa-2x"></i></a>
    </div>
    <div data-options="region:'south',border:false" style="height:30px;">
        <div id="copyRight">
            版权信息
        </div>
    </div>
    <div data-options="region:'west',split:true" title="<center></center>" style="width:235px;height:80%;">
        <ul id="treeMenu" class="easyui-tree" data-options="state:'close'">
            <li iconCls="fa fa-user"><span><a href="/Stuhome/Account">个人信息</a></span></li>
            <!--li iconCls="fa fa-key"><span><a href="s_retrievepwd.html">修改密码</a></span></li>
            <li iconCls="fa fa-comments"><span><a href="s_notice.html">通知公告</a></span></li-->
            <li iconCls="fa fa-pencil">
                <span><a href="#" style="margin: 0 5px">课后练习</a></span>
                <ul>
                    <li></li>
                </ul>
            </li>
            <li iconCls="fa fa-graduation-cap">
                <span><a href="#" style="margin: 0 5px">测评考试</a></span>
                <ul>
                    <li></li>
                </ul>
            </li>
            <li iconCls="fa fa-graduation-cap">
                <span><a href="#" style="margin: 0 5px">已完成考试</a></span>
                <ul>
                    <li></li>
                </ul>
            </li>
            <!--li iconCls="fa fa-bullhorn"><span><a href="s_feedback.html" target="myFrame">答疑解惑</a></span></li-->
        </ul>
    </div>
    <div id="center" data-options="region:'center'" style="background-color:#f2f2f2;width:auto;height:80%;padding: 10px;">
        @RenderBody()
    </div>
</body>
</html>
